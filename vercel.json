{
  "version": 2,
  "builds": [
      {
          "src": "dist/server.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["dist/**"] }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "dist/server.js"
      }
  ], 
  "env": {
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
  },
  "functions": [{
    "api/generate-pdf": {
      "timeout": 30}}
  ]
}